{
    "servers": {
        "stdlib": {
            "type": "stdio",
            "command": "uv",
            "args": [
                "run",
                "src/mcp_server_stdlib.py"
            ],
            "dev": {
                "watch": "src/mcp_server_stdlib/*.py"
            }
        },
        "django-mcp": {
            "url": "http://localhost:8000/mcp/"
        },
    }
}
